<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark">

    <fx:Metadata>
    	[HostComponent("spark.components.Button")]
    </fx:Metadata> 
    
    <s:states>
        <s:State name="up"/>
        <s:State name="over"/>
        <s:State name="down"/>
        <s:State name="disabled"/>
    </s:states>

    <!-- background -->
    <s:Rect x="0.5" y="0.5" width="14" height="27">
        <s:stroke>
            <s:SolidColorStroke color="#3D5D92" weight="1"/>
        </s:stroke>
        <s:fill>
            <s:SolidColor color="#FFFFFF"/>
        </s:fill>
    </s:Rect>

    <!-- Gripper -->
    <s:Group y="9.0" x="4.0">
        <s:Rect width="6" height="7">
            <s:fill>
                <s:SolidColor color="#FFFFFF"/>
            </s:fill>
        </s:Rect>
        <s:Line yFrom="1" yTo="1" xFrom="0" xTo="6">
            <s:stroke>
                <s:SolidColorStroke color="#3D5D92" pixelHinting="true" weight="1"/>
            </s:stroke>
        </s:Line>
        <s:Line yFrom="4" yTo="4" xFrom="0" xTo="6">
            <s:stroke>
                <s:SolidColorStroke color="#3D5D92" pixelHinting="true" weight="1"/>
            </s:stroke>
        </s:Line>
        <s:Line yFrom="7" yTo="7" xFrom="0" xTo="6">
            <s:stroke>
                <s:SolidColorStroke color="#3D5D92" pixelHinting="true" weight="1"/>
            </s:stroke>
        </s:Line>
    </s:Group>

</s:Skin>
